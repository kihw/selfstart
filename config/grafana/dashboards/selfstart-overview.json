{
  "dashboard": {
    "id": null,
    "title": "SelfStart Overview",
    "tags": ["selfstart", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Containers Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(selfstart_containers_total)",
            "legendFormat": "Total Containers"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "System Resources",
        "type": "graph", 
        "targets": [
          {
            "expr": "rate(cpu_usage_total[5m])",
            "legendFormat": "CPU Usage"
          },
          {
            "expr": "memory_usage_percent",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      }
    ],
    "schemaVersion": 27,
    "version": 1
  }
}